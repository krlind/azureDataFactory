{
	"name": "movieEtlPipeline",
	"properties": {
		"activities": [
			{
				"name": "Get Data From HTTP",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "GitGetCsv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "MoviesS3",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "movieDataFlowETL",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "movieDataFlowETL",
						"type": "DataFlowReference"
					}
				}
			},
			{
				"name": "movieDataFlowETL1",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "movieDataFlowETL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "movieDataFlowETL",
						"type": "DataFlowReference"
					}
				}
			}
		],
		"annotations": []
	}
